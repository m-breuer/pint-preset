{
    "name": "m-breuer/pint-preset",
    "description": "An opinionated Laravel Pint configuration preset for consistent PHP and Laravel project styling.",
    "keywords": [
        "laravel",
        "pint",
        "php-cs-fixer",
        "code-style",
        "formatter",
        "preset"
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/pint": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Marcel Breuer"
        }
    ]
}
